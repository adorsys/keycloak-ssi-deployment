    "vc.credential_configuration_id": "KMACredential",
    "vc.credential_identifier": "KMACredential",
    "vc.format": "dc+sd-jwt",
    "vc.expiry_in_seconds": "31536000",
    "include.in.token.scope": "true",
    "vc.verifiable_credential_type": "person_vct",
    "vc.supported_credential_types": "person_vct",
    "vc.credential_contexts": "person_vct",
    "vc.cryptographic_binding_methods_supported": "jwk",
    "vc.proof_signing_alg_values_supported": "ES256",
    "vc.credential_signing_alg_values_supported": "ES256",
    "vc.display": "[{\"locale\":\"de-DE\",\"name\":\"Person Credential\",\"logo\":{\"uri\":\"https://kci-portal.solutions.adorsys.com/credential_files/stbk-wl-icon.png\",\"alt_text\":\"Person Credential\"},\"background_color\":\"#f0f0f0\",\"text_color\":\"#000000\"},{\"locale\":\"en-US\",\"name\":\"Person Credential\",\"logo\":{\"uri\":\"https://kci-portal.solutions.adorsys.com/credential_files/stbk-wl-icon.png\",\"alt_text\":\"Person Credential\"},\"background_color\":\"#f0f0f0\",\"text_color\":\"#000000\"}]",
    "vc.issuer_did": "https://localhost:8443/realms/oid4vc-vci",
    "vc.sd_jwt.number_of_decoys": "2",
    "vc.credential_build_config.sd_jwt.visible_claims": "id,iat,nbf,exp,jti,status",
    "vc.credential_build_config.hash_algorithm": "sha-256",
    "vc.credential_build_config.token_jws_type": "dc+sd-jwt",
    "vc.include_in_metadata": "true"
  },
